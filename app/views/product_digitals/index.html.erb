<p id="notice"><%= notice %></p>

<h1>Product Digitals</h1>

<table>
  <thead>
    <tr>
      <th>Products</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_digitals.each do |product_digital| %>
      <tr>
        <td><%= product_digital.products %></td>
        <td><%= link_to 'Show', product_digital %></td>
        <td><%= link_to 'Edit', edit_product_digital_path(product_digital) %></td>
        <td><%= link_to 'Destroy', product_digital, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Digital', new_product_digital_path %>
