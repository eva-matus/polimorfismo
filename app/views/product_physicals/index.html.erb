<p id="notice"><%= notice %></p>

<h1>Product Physicals</h1>

<table>
  <thead>
    <tr>
      <th>Products</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_physicals.each do |product_physical| %>
      <tr>
        <td><%= product_physical.products %></td>
        <td><%= link_to 'Show', product_physical %></td>
        <td><%= link_to 'Edit', edit_product_physical_path(product_physical) %></td>
        <td><%= link_to 'Destroy', product_physical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Physical', new_product_physical_path %>
